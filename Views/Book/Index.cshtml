
@model List<BookStoreMVCDemo.Models.Domain.BookModel> 

<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Name</th>
      <th scope="col">Description</th>
      <th scope="col">Category</th>
      <th scope="col">Price</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
        @foreach(var BookModel in Model)
        {
        <tr>
            <th scope="row">@BookModel.Id</th>
            <td>@BookModel.Name</td>
            <td>@BookModel.Description</td>
            <td>@BookModel.Category</td>
            <td>@BookModel.Price</td>
            <td><a href="/Book/View/@BookModel.Id">edit</a></td>
        </tr>   
       } 
  </tbody>
</table>